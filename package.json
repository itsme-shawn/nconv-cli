{
  "name": "nconv-cli",
  "version": "1.0.5",
  "description": "A CLI tool that automatically convert Notion pages into Markdown, HTML, and PDF.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "nconv": "./bin/nconv.js"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "bin",
    "patches",
    ".env.example"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "start": "node dist/index.js",
    "postinstall": "patch-package"
  },
  "keywords": [
    "notion",
    "markdown",
    "blog",
    "converter",
    "cli"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsme-shawn/nconv-cli.git"
  },
  "homepage": "https://github.com/itsme-shawn/nconv-cli#readme",
  "bugs": {
    "url": "https://github.com/itsme-shawn/nconv-cli/issues"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "axios": "^1.13.2",
    "chalk": "^5.6.2",
    "commander": "^12.0.0",
    "dotenv": "^17.2.3",
    "md-to-pdf": "^5.2.5",
    "notion-exporter": "^0.8.1",
    "ora": "^8.0.0",
    "patch-package": "^8.0.1",
    "prompts": "^2.4.2",
    "slugify": "^1.6.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "@types/prompts": "^2.4.9",
    "@types/uuid": "^10.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
